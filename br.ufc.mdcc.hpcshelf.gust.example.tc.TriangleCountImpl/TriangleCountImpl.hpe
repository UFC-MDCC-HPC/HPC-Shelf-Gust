utput = null;&#xA;&#xA;&#x9;&#x9;public IIterator&lt;IKVPair&lt;IVertex, IDataTriangle>> Output&#xA;&#x9;&#x9;{&#xA;&#x9;&#x9;&#x9;get&#xA;&#x9;&#x9;&#x9;{&#xA;&#x9;&#x9;&#x9;&#x9;if (this.output == null)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;this.output = (IIterator&lt;IKVPair&lt;IVertex, IDataTriangle>>) Services.getPort(&quot;output&quot;);&#xA;&#x9;&#x9;&#x9;&#x9;return this.output;&#xA;&#x9;&#x9;&#x9;}&#xA;&#x9;&#x9;}&#xA;&#x9;}&#xA;}" name="BaseITriangleCountImpl.cs" srcType="base" uri="br.ufc.mdcc.hpcshelf.gust.example.tc.TriangleCountImpl/bin/1.0.0.0/BaseITriangleCountImpl.dll" versionId="1.0.0.0">
